{
  "manifest_version": 2,
  "name": "MyIAControl",
  "version": "1.1",

  "description": "My Internet Anxiety Control (MyIAControl) tool for some sites",

  "icons": {
    "48": "icons/extension_icon.png"
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["myiacontrol.js"]
    }
  ],

  "options_ui": {
    "page": "options.html"
  },

  "permissions": ["storage"]
}
